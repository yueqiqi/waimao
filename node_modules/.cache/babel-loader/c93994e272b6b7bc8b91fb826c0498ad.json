=========== -->\r\n    <!-- 收货 -->\r\n    <div v-show=\"sh1\">\r\n      <utake></utake>\r\n    </div>\r\n    <!-- ================================================================================= -->\r\n    <!-- 结算汇总 -->\r\n    <div v-show=\"js1\">\r\n      <uclosing></uclosing>\r\n    </div>\r\n    <!-- ================================================================================= -->\r\n\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n//这里可以导入其他文件（比如：组件，工具js，第三方插件js，json文件，图片文件等等）\r\n//例如：import 《组件名称》 from '《组件路径》';\r\n// 进口订单准入\r\nimport orders from './orders'\r\n// 合同签订\r\nimport usign from './compact'\r\n// 付款开证\r\nimport upayment from './receiving'\r\n// 运输清关\r\nimport ucarriage from './carriage'\r\n// \r\nimport urate from './rate'\r\n// 收货\r\nimport utake from './take'\r\nimport uclosing from './closing'\r\nexport default{\r\n  components:{\r\n    orders,\r\n    usign,\r\n    upayment,\r\n    ucarriage,\r\n    urate,\r\n    utake,\r\n    uclosing\r\n  },\r\n  data(){\r\n    return{\r\n      // 步骤条++\r\n      actives:0,\r\n\r\n       m2:\"normal\",\r\n      m3:\"normal\",\r\n      m4:\"normal\",\r\n      m5:\"normal\",\r\n      m6:\"normal\",\r\n      m7:\"normal\",\r\n      m8:\"normal\",\r\n      active:\"active\",\r\n      add1:false,\r\n      ins1:true,\r\n      ht1:false,\r\n      fk1:false,\r\n      ts1:false,\r\n      ys1:false,\r\n      sh1:false,\r\n      js1:false,\r\n  // 订单号\r\n  orderNum:\"20190729999\",\r\n  // 客户类型\r\n  userType:\"一般业务客户\"\r\n    }\r\n  },\r\n   methods:{\r\n    add(){\r\n      console.log(\"新增订单\")\r\n      // this.ins1=false\r\n      // this.add1=true\r\n      // this.ht1=false\r\n      // this.fk1=false\r\n      // this.ts1=false\r\n      // this.ys1=false\r\n      // this.sh1=false\r\n      // this.js1=false\r\n    },\r\n    ins(){\r\n      console.log(\"订单准入\")\r\n      this.ins1=true\r\n      this.add1=false\r\n      this.ht1=false\r\n      this.fk1=false\r\n      this.ts1=false\r\n      this.ys1=false\r\n      this.sh1=false\r\n      this.js1=false\r\n      this.m2=1\r\n      // this.actives+=1\r\n    },\r\n    ht(){\r\n      console.log(\"合同签订\")\r\n      this.ins1=false\r\n      this.add1=false\r\n      this.ht1=true\r\n      this.fk1=false\r\n      this.ts1=false\r\n      this.ys1=false\r\n      this.sh1=false\r\n      this.js1=false\r\n      this.m3=1\r\n      this.actives=1\r\n      console.log(\"步骤条\"+this.actives)\r\n    },\r\n    fk(){\r\n      console.log(\"付款开征\")\r\n      this.ins1=false\r\n      this.add1=false\r\n      this.ht1=false\r\n      this.fk1=true\r\n      this.ts1=false\r\n      this.ys1=false\r\n      this.sh1=false\r\n      this.js1=false\r\n      this.m4=1\r\n      this.actives=2\r\n      console.log(\"步骤条\"+this.actives)\r\n    },\r\n    ts(){\r\n      console.log(\"退税\")\r\n      this.ins1=false\r\n      this.add1=false\r\n      this.ht1=false\r\n      this.fk1=false\r\n      this.ts1=true\r\n      this.ys1=false\r\n      this.sh1=false\r\n      this.js1=false\r\n      this.m5=1\r\n      this.actives=4\r\n      console.log(\"步骤条\"+this.actives)\r\n    },\r\n    ys(){\r\n      console.log(\"运输清关\")\r\n      this.ins1=false\r\n      this.add1=false\r\n      this.ht1=false\r\n      this.fk1=false\r\n      this.ts1=false\r\n      this.ys1=true\r\n      this.sh1=false\r\n      this.js1=false\r\n      this.m6=1\r\n      this.actives=3\r\n      console.log(\"步骤条\"+this.actives)\r\n    },\r\n    sh(){\r\n      console.log(\"收货\")\r\n      this.ins1=false\r\n      this.add1=false\r\n      this.ht1=false\r\n      this.fk1=false\r\n      this.ts1=false\r\n      this.ys1=false\r\n      this.sh1=true\r\n      this.js1=false\r\n      this.m7=1\r\n      this.actives=5\r\n      console.log(\"步骤条\"+this.actives)\r\n    },\r\n    js(){\r\n      console.log(\"结算汇总\")\r\n      this.ins1=false\r\n      this.add1=false\r\n      this.ht1=false\r\n      this.fk1=false\r\n      this.ts1=false\r\n      this.ys1=false\r\n      this.sh1=false\r\n      this.js1=true\r\n      this.m8=1\r\n      this.actives=6\r\n      console.log(\"步骤条\"+this.actives)\r\n    },\r\n   }\r\n}\r\n</script>\r\n<style scoped>\r\n/* 添加新用户 */\r\n.adduser{\r\n  background:#fff;\r\n  width:1644px;height:70px;\r\n  margin-top:21px;\r\n  border-left:6px solid rgba(6,102,164,1);\r\n}\r\n.img{\r\n  width:31px;height: 31px;margin:auto 0;\r\n  margin-left:16px;\r\n}\r\n.title{\r\n  font-size:24px;\r\n  font-family:Microsoft YaHei;\r\n  font-weight:bold;\r\n  color:rgba(51,51,51,1);\r\n  line-height:70px;\r\n  margin-left:10px;\r\n}\r\n.title2{\r\n  font-size:20px;\r\nfont-family:Microsoft YaHei;\r\nfont-weight:bold;\r\ncolor:rgba(51,51,51,1);\r\nline-height:70px;\r\nmargin-left:38px;\r\n}\r\n.blue{\r\n  font-size:20px;\r\n  margin-left:8px;\r\n  margin-right:2px;\r\nfont-family:Microsoft YaHei;\r\nfont-weight:bold;\r\ncolor:rgba(6,102,164,1);\r\nline-height:70px;\r\n}\r\n/* 大白框 */\r\n.border{\r\n  width:1649px;\r\nheight:1427px;\r\nbackground:rgba(255,255,255,1);\r\nmargin-top:10px;\r\npadding-left:57px;\r\n}\r\n/* 订单流程 */\r\n.order{\r\n  font-size:20px;\r\nfont-family:Microsoft YaHei;\r\nfont-weight:bold;\r\ncolor:rgba(51,51,51,1);\r\npadding-top:40px;\r\nmargin-bottom:20px;\r\n}\r\n/* 步骤条 */\r\n.steps{\r\n  width:1493px;\r\n  border-bottom:2px solid rgba(204,204,204,1);\r\n}\r\n.boxzxc{\r\nwidth:100px;\r\nheight:30px;\r\nbackground:rgba(154,195,220,1);\r\nborder-radius:10px;\r\ntext-align: center;\r\nline-height: 30px;\r\nfont-weight:400;\r\ncolor:rgba(255,255,255,1);\r\ncursor:default \r\n}\r\n.normal{\r\nwidth:100px;\r\nheight:30px;\r\nbackground:rgba(237,237,237,1);\r\nborder-radius:10px;\r\ntext-align: center;\r\nline-height: 30px;\r\nfont-weight:400;\r\ncolor:#333;\r\ncursor:default \r\n}\r\n.active{\r\n  width:100px;\r\nheight:30px;\r\nbackground:rgba(6,102,164,1);\r\nborder-radius:10px;\r\ntext-align: center;\r\nline-height: 30px;\r\nfont-weight:400;\r\ncolor:rgba(255,255,255,1);\r\ncursor:default \r\n}\r\n\r\n.line{\r\n  width:50px;\r\nheight:2px;\r\nbackground:rgba(154,195,220,1);\r\nmargin-top:15px;\r\nmargin-left:10px;margin-right:10px;\r\n}\r\n/* 曲线 */\r\n.curve{\r\n   width: 15px; \r\n  height: 40px;  \r\n  border: solid 2px rgba(154,195,220,1);\r\n  /* border-color: #000 transparent transparent transparent;\r\n  border-radius: 50%/10px 10px 0 0; */\r\n  /* border-color: */\r\n  border-radius: 10px 0px 0px 10px;\r\n  margin-top:-4px;\r\n  border-right: 0;\r\n  margin-right:14px;\r\n}\r\n.curve2{\r\n   width: 15px; \r\n  height: 40px;  \r\n  border: solid 2px rgba(154,195,220,1);\r\n  /* border-color: #000 transparent transparent transparent;\r\n  border-radius: 50%/10px 10px 0 0; */\r\n  /* border-color: */\r\n  border-radius: 0px 10px 10px 0px;\r\n  margin-top:14px;\r\n  border-left: 0;\r\n  /* margin-right:14px; */\r\n  margin-left:14px;\r\n}\r\n.dis{\r\n  pointer-events:none;\r\n}\r\n</style>"],"sourceRoot":"src\\components\\waimao\\orderlist\\import"}]}