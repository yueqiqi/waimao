条\" + this.actives);\n    },\n    ys: function ys() {\n      console.log(\"运输清关\");\n      this.ins1 = false;\n      this.add1 = false;\n      this.ht1 = false;\n      this.fk1 = false;\n      this.ts1 = false;\n      this.ys1 = true;\n      this.sh1 = false;\n      this.js1 = false;\n      this.m6 = 1;\n      this.actives = 3;\n      console.log(\"步骤条\" + this.actives);\n    },\n    sh: function sh() {\n      console.log(\"收货\");\n      this.ins1 = false;\n      this.add1 = false;\n      this.ht1 = false;\n      this.fk1 = false;\n      this.ts1 = false;\n      this.ys1 = false;\n      this.sh1 = true;\n      this.js1 = false;\n      this.m7 = 1;\n      this.actives = 5;\n      console.log(\"步骤条\" + this.actives);\n    },\n    js: function js() {\n      console.log(\"结算汇总\");\n      this.ins1 = false;\n      this.add1 = false;\n      this.ht1 = false;\n      this.fk1 = false;\n      this.ts1 = false;\n      this.ys1 = false;\n      this.sh1 = false;\n      this.js1 = true;\n      this.m8 = 1;\n      this.actives = 6;\n      console.log(\"步骤条\" + this.actives);\n    }\n  }\n};",{"version":3,"sources":["mainsteps.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyFA;AACA;AACA;AACA,OAAA,MAAA,MAAA,UAAA,C,CACA;;AACA,OAAA,KAAA,MAAA,WAAA,C,CACA;;AACA,OAAA,QAAA,MAAA,aAAA,C,CACA;;AACA,OAAA,SAAA,MAAA,YAAA,C;;AAEA,OAAA,KAAA,MAAA,QAAA,C,CACA;;AACA,OAAA,KAAA,MAAA,QAAA;AACA,OAAA,QAAA,MAAA,WAAA;AACA,eAAA;AACA,cAAA;AACA,kBADA;AAEA,gBAFA;AAGA,sBAHA;AAIA,wBAJA;AAKA,gBALA;AAMA,gBANA;AAOA;AAPA,GADA;AAUA,MAVA,kBAUA;AACA,WAAA;AACA;AACA,eAAA,CAFA;AAIA,UAAA,QAJA;AAKA,UAAA,QALA;AAMA,UAAA,QANA;AAOA,UAAA,QAPA;AAQA,UAAA,QARA;AASA,UAAA,QATA;AAUA,UAAA,QAVA;AAWA,cAAA,QAXA;AAYA,YAAA,KAZA;AAaA,YAAA,IAbA;AAcA,WAAA,KAdA;AAeA,WAAA,KAfA;AAgBA,WAAA,KAhBA;AAiBA,WAAA,KAjBA;AAkBA,WAAA,KAlBA;AAmBA,WAAA,KAnBA;AAoBA;AACA,gBAAA,aArBA;AAsBA;AACA,gBAAA;AAvBA,KAAA;AAyBA,GApCA;AAqCA,WAAA;AACA,OADA,iBACA;AACA,cAAA,GAAA,CAAA,MAAA,EADA,CAEA;AACA;AA