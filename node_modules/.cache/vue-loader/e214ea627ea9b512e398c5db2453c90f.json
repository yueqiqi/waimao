\r\n              picker.$emit('pick', new Date());\r\n            }\r\n          }, {\r\n            text: '昨天',\r\n            onClick(picker) {\r\n              const date = new Date();\r\n              date.setTime(date.getTime() - 3600 * 1000 * 24);\r\n              picker.$emit('pick', date);\r\n            }\r\n          }, {\r\n            text: '一周前',\r\n            onClick(picker) {\r\n              const date = new Date();\r\n              date.setTime(date.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit('pick', date);\r\n            }\r\n          }]\r\n        },\r\n  // 是否禁用\r\nmybtn:true,\r\n// 起始时间\r\n  stime:\"\",\r\n  // 结束时间\r\n  etime:\"\",\r\n  // 搜索电话号码保存\r\n  searchphone:\"\",\r\n  // 搜索用户名保存\r\n  searchuser:\"\",\r\n};\r\n},\r\n//监听属性 类似于data概念\r\ncomputed: {\r\n  //分页\r\n    indexs: function(){\r\n    var left = 1;\r\n    var right = this.allpage;\r\n    var ar = [];\r\n    if(this.allpage>= 5){\r\n    if(this.cur > 3 && this.cur < this.allpage-2){\r\n    left = this.cur - 2\r\n    right = this.cur + 2\r\n    }else{\r\n    if(this.cur<=3){\r\n    left = 1\r\n    right = 5\r\n    }else{\r\n    right = this.allpage\r\n    left = this.allpage -4\r\n    }\r\n    }\r\n    }\r\n    while (left <= right){\r\n    ar.push(left)\r\n    left ++\r\n    }\r\n    return ar\r\n    },\r\n\r\n},\r\n//监控data中的数据变化\r\nwatch: {\r\n\r\n},\r\n//方法集合\r\nmethods: {\r\n  // 状态选择\r\n  statu(opt){\r\n    console.log(opt)\r\n  },\r\n  bus(dat){\r\n    console.log(dat)\r\n  },\r\n   // 888888888888888888888888888888888888888888888888888888888888\r\n  pageSkip(){\r\n\t\t\tvar maxPage = this.allpage  ;\r\n\t\t\tvar skipPage = Number(document.getElementsByClassName(\"jumppage\")[0].value);\r\n\t\t\tconsole.log(typeof skipPage);\r\n\t\t\tif(!skipPage){\r\n\t\t\t\talert(\"请输入跳转页码\");\r\n\t\t\t\treturn;\r\n\t\t\t}else if(skipPage<1 || skipPage>maxPage){\r\n\t\t\t\talert(\"您输入的页码超过页数范围了！\");\r\n\t\t\t\treturn;\r\n\t\t\t}else{\r\n\t\t\t\t//this.cur=skipPage;\r\